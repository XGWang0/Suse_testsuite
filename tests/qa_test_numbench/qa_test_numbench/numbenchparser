BEGIN {
        push (@INC,"/usr/share/qa/lib");
}
use benchxml;
my $bench_results = {};
$bench_results->{attrs} = {
                                depth => {
                                                id => 'depth',
                                                label => 'depth',
                                                desc => 'Iterative Depth',
                                                type => 'linear',
                                                unit => 'depth'
                                        },
                               time => {
                                                id => 'time',
                                                label => 'time',
                                                desc => 'CPU comsumption measured by time.',
                                                type => 'linear',
                                                unit => 'sec'
                                        }
};
$bench_result->{values}=[];
$bench_result->{graphs}=[];
my $val;
my $graphs;
my $v=0;
my $g=0;
my $depth;
my $name;
#my $x;
#my $y;
foreach my $row (<STDIN>)
{
    if( $row =~ /((fibonacci|pi|primo|collatz|oro)\s+iteration)/ )
    {
#        $x='times'.$g;
#	$y='CPUTime'.$g;
        $name = $1;
        $graphs->[$g]->{label} = $1;
        $graphs->[$g]->{desc} = $1;
        $graphs->[$g]->{result} = 'time';
        $graphs->[$g]->{axis_1} = 'depth';
        $g = $g+1;
    }
    if( $row =~ /^([\d]+)\s+([\d]+[\.]?[\d]*)/ )
    {
        $val->[$v]->{depth} = $1;
        $val->[$v]->{time} = $2;
        $v++;
    }
}
$bench_results->{values} = $val;
$bench_results->{graphs} = $graphs;
print bench_data_to_xml($bench_results);


